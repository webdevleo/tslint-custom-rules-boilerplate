{
  "name": "tslint-fanhub-rules",
  "version": "1.1.0",
  "description": "A template for developing a custom TSLint rule, using unit testing.",
  "scripts": {
    "build": "tsc",
    "publish": "npm pack",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webdevleo/tslint-fanhub-rules.git"
  },
  "devDependencies": {
    "@types/jest": "^25.1.3",
    "jest": "^25.1.0",
    "ts-jest": "^25.2.1",
    "typescript": "^3.8.2",
    "tsutils": "^3.17.1"
  },
  "dependencies": {
    "tslint": "^6.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/**/*.spec.(ts|tsx)"
    ],
    "testEnvironment": "node"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/webdevleo/tslint-fanhub-rules/issues"
  },
  "homepage": "https://github.com/webdevleo/tslint-fanhub-rules#readme",
  "main": "tslint-fanhub-rules.json"
}
